CREATE TABLE store (id INTEGER PRIMARY KEY,  name TEXT, quantity INTEGER, price INTEGER);

INSERT INTO store VALUES (1, "Blank Tee",4,10);
INSERT INTO store VALUES (2, "Cropped Fitted Shirt",3,15);
INSERT INTO store VALUES (3, "Baggy Jeans",4,15);
insert into store values (4, "Tote bag",8,10);
insert into store values (5, "Sambas", 2,60);
SELECT * FROM store order by price;
select sum(price) from store;
